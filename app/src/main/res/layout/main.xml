<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">
	<TableLayout android:layout_width="match_parent" android:id="@+id/main_linear1" android:layout_height="wrap_content">
		<TableRow>
			<Button android:fontFamily="@font/u5cdot" android:text="@string/main_setup_configure_imes" android:textAllCaps="false"
				android:id="@+id/main_setup_btn_configure_imes" android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
			<Button android:fontFamily="@font/u5cdot" android:text="@string/main_setup_input_lang" android:textAllCaps="false" android:id="@+id/main_setup_btn_input_lang" android:layout_width="wrap_content" android:layout_height="wrap_content" />
		</TableRow>
		<TableRow>
			<Button android:fontFamily="@font/u5cdot" android:text="@string/main_setup_set_ime" android:textAllCaps="false" android:id="@+id/main_setup_btn_set_ime" android:layout_width="wrap_content" android:layout_height="wrap_content" />
			<Button android:fontFamily="@font/u5cdot" android:text="@string/main_setup_get_dicts" android:textAllCaps="false" android:id="@+id/main_setup_btn_get_dicts" android:layout_width="wrap_content" android:layout_height="wrap_content" />
		</TableRow>
		<TableRow>
			<Button android:fontFamily="@font/u5cdot" android:id="@+id/main_setup_btn_settings" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/main_setup_settings" android:textAllCaps="false" />
			<EditText android:fontFamily="@font/u5cdot" android:text="@string/main_setup_tAst" android:id="@+id/main_setup_edit_test" android:layout_width="fill_parent" android:gravity="center" android:layout_height="wrap_content" />
		</TableRow>
	</TableLayout>
	<TextView android:fontFamily="@font/u5cdot" android:layout_height="wrap_content" android:id="@+id/main_description" android:layout_width="wrap_content" />
</LinearLayout>
