<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LatinKeyboardBaseView">
        <attr name="keyboardViewStyle" format="reference" />
        <attr name="keyBackground" format="reference" />
        <attr name="keyTextSize" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="keyTextColor" format="color" /><attr name="keyHintColor" format="color" />
        <attr name="shadowColor" format="color" /><attr name="keyCursorColor" format="color" />
        <attr name="invertSymbols" format="boolean" />
        <attr name="recolorSymbols" format="boolean" />
        <attr name="backgroundAlpha" format="integer" />
        <attr name="keyPreviewLayout" format="reference" />
        <attr name="keyPreviewOffset" format="dimension" />
        <attr name="keyPreviewHeight" format="dimension" />
        <attr name="keyHysteresisDistance" format="dimension" />
        <attr name="verticalCorrection" format="dimension" />
        <attr name="popupLayout" format="reference" />
        <attr name="shadowRadius" format="float" />
        <attr name="backgroundDimAmount" format="float" />
        <attr name="keyTextStyle"><flag name="normal" value="0" /><flag name="bold" value="1" /><flag name="italic" value="2" /></attr>
        <attr name="symbolColorScheme"><flag name="white" value="0" /><flag name="black" value="1" /></attr>
    </declare-styleable>
</resources>
