<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res-auto" android:keyWidth="10%p" android:horizontalGap="0px" android:verticalGap="@dimen/key_bottom_gap">
    <Row android:rowEdgeFlags="top">
        <Key android:keyLabel="1" android:popupCharacters="|" android:keyEdgeFlags="left" />
        <Key android:keyLabel="2" android:popupCharacters="+" />
        <Key android:keyLabel="3" android:popupCharacters="-" />
        <Key android:keyLabel="4" android:popupCharacters="F" />
        <Key android:keyLabel="5" android:popupCharacters="R" />
        <Key android:keyLabel="6" android:popupCharacters="=" />
        <Key android:keyLabel="7" android:popupCharacters="%" />
        <Key android:keyLabel="8" android:popupCharacters="/" />
        <Key android:keyLabel="|" android:popupCharacters="*" />
        <Key android:keyLabel="^" android:popupCharacters="" android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:keyLabel="9" android:popupCharacters="" android:keyEdgeFlags="left" />
        <Key android:keyLabel="L" android:popupCharacters="|&lt;" />
        <Key android:keyLabel="J" android:popupCharacters="" />
        <Key android:keyLabel="q" android:popupCharacters="" />
        <Key android:keyLabel="w" android:popupCharacters="" />
        <Key android:keyLabel="X" android:popupCharacters="" />
        <Key android:keyLabel="F" android:popupCharacters="" />
        <Key android:keyLabel="0" android:popupCharacters="" />
        <Key android:keyLabel="~" android:popupCharacters="" />
        <Key android:keyLabel="&amp;" android:popupCharacters="" />
        <Key android:keyLabel=")" android:popupCharacters="" android:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key android:codes="@integer/key_shift" android:keyLabel="sel" android:keyWidth="10%p" android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" />
        <Key android:keyLabel="+" android:popupCharacters="S@&gt;-&lt;" />
        <Key android:keyLabel="-" android:popupCharacters="L|/\\" />
        <Key android:keyLabel="*" android:popupCharacters="V^wW" />
        <Key android:keyLabel="F" android:popupCharacters="M^*X-+\/*=S()[]{}" />
        <Key android:keyLabel="R" android:popupCharacters="1-23456789LJqwXF.+-*/=^S" />
        <Key android:keyLabel="/" android:popupCharacters="\UMZ%S" />
        <!--        <Key android:keyLabel="." android:popupKeyboard="@xml/popup_punctuation" android:isModifier="true" />-->
        <Key android:keyLabel="." android:popupCharacters=";:,~`'&quot;~`-_=+*" android:isModifier="true" />
        <Key android:keyLabel="=" android:popupCharacters="&lt;&gt;CGEZ" />
        <Key android:codes="@integer/key_delete" android:keyIcon="@drawable/sym_keyboard_delete" android:iconPreview="@drawable/sym_keyboard_feedback_delete" android:keyWidth="10%p" android:isModifier="true" android:isRepeatable="true" android:keyEdgeFlags="right" />
    </Row>
    <Row android:keyboardMode="@+id/mode_symbols" android:rowEdgeFlags="bottom"> <!-- <Key android:codes="@integer/key_f1" android:isModifier="true" /> -->
        <Key android:codes="@integer/key_symbol" android:keyLabel="@string/label_alpha_key" android:keyWidth="10%p" android:isModifier="true" android:keyEdgeFlags="left" />
        <Key android:codes="@integer/key_tab" android:keyLabel="tAb"/>
        <!--        <Key android:codes="@integer/kii_lminu" android:keyIcon="@drawable/simbql_kibord_lminu" />-->
        <Key android:codes="@integer/key_alt_left" android:keyLabel="alt" android:isSticky="true" android:isModifier="true" />
        <Key android:keyLabel=" " android:popupCharacters="\?=12+.3456789LJqwXF0+-*fR/%S:;CG&amp;|WV" android:keyWidth="20%p" android:isModifier="true" />
        <Key android:codes="@integer/kii_qndqrskor" android:keyIcon="@drawable/simbl_undrskor" android:popupCharacters='|-=~!\\/' />
        <!--        <Key android:codes="@integer/kii_rminu" android:keyIcon="@drawable/simbql_kibord_rminu" />-->
        <Key android:codes="@integer/key_ctrl_left" android:keyLabel="ktl" android:isSticky="true" android:keyEdgeFlags="left" />
        <Key android:keyLabel="/" android:popupCharacters="\\%?!^`'&quot;~|" />
        <Key android:keyLabel="#" android:popupCharacters="1-23456789LJqwXF0.+-*/=^S|.,^%*-+!" />
        <Key android:codes="@integer/key_return" android:keyIcon="@drawable/sym_keyboard_return" android:iconPreview="@drawable/sym_keyboard_feedback_return" android:isModifier="true"
            android:keyWidth="10%p" android:keyEdgeFlags="right" />
    </Row>
